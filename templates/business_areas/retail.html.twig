        <div class="row py-5">
            <div class="col-12 col-md-6 d-flex align-items-center">
                <div>
                    {{colRow.text[0]|raw}}
                </div>
            </div>
        </div>
    <!--/3ª ROW-->
    <!--4ª ROW-->
    {% set colRow = colContent['fourth-row'] %}
    <div class="row d-flex justify-content-end pt-5">
            <div class="col-12 col-md-6">
                <div class="py-5 py-md-2">
                    {{colRow.text[0]|raw}}
                </div>
            </div>
            <div class="col-12">
                <svg class='clothing-svg-line-window_2'>
                    <line x1="500" y1="0" x2="0" y2="250" style="stroke:rgb(115,115,115);stroke-width:1" />
